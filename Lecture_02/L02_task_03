# zadanie 3

def char_frequency(str=''):
    if str:
      freq = {}
      for c in str:
        freq[c] = str.count(c)
      return freq
    else:
      return {}
  

assert char_frequency("576") == {'5': 1, '6': 1, '7': 1}
assert char_frequency() == {}

assert char_frequency('Ala ma kota, a kot kocha ale') == \
    {'e': 1, 'm': 1, 'a': 6, 'o': 3, 'k': 3, 'A': 1, 't': 2,
     ',': 1, ' ': 6, 'l': 2, 'c': 1, 'h': 1}
